{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh3/main/themes/schema.json",
    "final_space": true,
    "console_title": true,
    "console_title_style": "template",
    "console_title_template": "{{if .Root}} :: root :: {{end}}{{.Folder}} :: {{.Path}}",
    "blocks": [
        {
            "type": "prompt",
            "alignment": "left",
            "segments": [
                {
                    "type": "os",
                    "style": "plain",
                    "foreground": "#FFF",
                    "properties": {
                        "postfix": " \u276F"
                    }
                },
                {
                    "type": "root",
                    "style": "plain",
                    "foreground": "#B5B50D",
                    "properties": {
                        "postfix": ""
                    }
                },
                {
                    "type": "git",
                    "style": "plain",
                    "foreground": "#C2C206",
                    "properties": {
                        "postfix": "",
                        "display_stash_count": true,
                        "display_upstream_icon": true
                    }
                },
                {
                    "type": "go",
                    "style": "plain",
                    "foreground": "#7FD5EA",
                    "properties": {
                        "prefix": " \uFCD1",
                        "postfix": ""
                    }
                },
                {
                    "type": "python",
                    "style": "plain",
                    "foreground": "#906CFF",
                    "properties": {
                        "prefix": " \uF81F",
                        "postfix": ""
                    }
                },
                {
                    "type": "node",
                    "style": "plain",
                    "foreground": "#6CA35E",
                    "properties": {
                        "prefix": " \uF898",
                        "postfix": ""
                    }
                },
                {
                    "type": "envvar",
                    "style": "plain",
                    "foreground": "#FFF",
                    "properties": {
                        "prefix": " \u2772<#888>\uE7B0 Machine Name</>\u276F ",
                        "postfix": " \u2773",
                        "var_name": "DOCKER_MACHINE_NAME"
                    }
                },
                {
                    "type": "envvar",
                    "style": "plain",
                    "foreground": "#FFF",
                    "properties": {
                        "prefix": " \u2772<#888>\uE7B0 Host</>\u276F ",
                        "postfix": " \u2773",
                        "var_name": "DOCKER_HOST"
                    }
                },
                {
                    "type": "envvar",
                    "style": "plain",
                    "foreground": "#FFF",
                    "properties": {
                        "prefix": " \u2772<#888>\uE7AD Profile</>\u276F ",
                        "postfix": " \u2773",
                        "var_name": "AWS_PROFILE"
                    }
                },
                {
                    "type": "envvar",
                    "style": "plain",
                    "foreground": "#FFF",
                    "properties": {
                        "prefix": " \u2772<#888>\uE7AD Region</>\u276F ",
                        "postfix": " \u2773",
                        "var_name": "AWS_DEFAULT_REGION"
                    }
                },
                {
                    "type": "envvar",
                    "style": "plain",
                    "foreground": "#FFF",
                    "properties": {
                        "prefix": " \u2772<#888>\uE7AD Output</>\u276F ",
                        "postfix": " \u2773",
                        "var_name": "AWS_DEFAULT_OUTPUT"
                    }
                },
                {
                    "type": "envvar",
                    "style": "plain",
                    "foreground": "#FFF",
                    "properties": {
                        "prefix": " \u2772<#888>\uE7AD CLI Auto Prompt</>\u276F ",
                        "postfix": " \u2773",
                        "var_name": "AWS_CLI_AUTO_PROMPT"
                    }
                },
                {
                    "type": "envvar",
                    "style": "plain",
                    "foreground": "#FFF",
                    "properties": {
                        "prefix": " \u2772<#888>\uE7AD Pager</>\u276F ",
                        "postfix": " \u2773",
                        "var_name": "AWS_PAGER"
                    }
                }
            ]
        },
        {
            "type": "prompt",
            "alignment": "right",
            "segments": [
                {
                    "type": "exit",
                    "style": "plain",
                    "foreground": "#C94A16"
                }
            ]
        },
        {
            "type": "newline"
        },
        {
            "type": "prompt",
            "alignment": "left",
            "segments": [
                {
                    "type": "path",
                    "style": "plain",
                    "foreground": "#CCC",
                    "properties": {
                        "prefix": " \uE602 ",
                        "style": "full"
                    }
                }
            ]
        },
        {
            "type": "prompt",
            "alignment": "right",
            "segments": [
                {
                    "type": "executiontime",
                    "style": "plain",
                    "foreground": "#888",
                    "properties": {
                        "always_enabled": true,
                        "style": "houston",
                        "postfix": "\uF250"
                    }
                }
            ]
        },
        {
            "type": "newline"
        },
        {
            "type": "prompt",
            "alignment": "left",
            "segments": [
                {
                    "type": "text",
                    "style": "plain",
                    "foreground": "#007ACC",
                    "properties": {
                        "prefix": "",
                        "text": "\uE602"
                    }
                }
            ]
        },
        {
            "type": "rprompt",
            "alignment": "right",
            "segments": [
                {
                    "type": "time",
                    "style": "plain",
                    "foreground": "#007ACC",
                    "properties": {
                        "time_format": "2006-01-02T15:04:05Z07:00",
                        "postfix": "\uF64F"
                    }
                }
            ]
        }
    ]
}
